{
  "name": "webpack-es6-demo",
  "version": "1.0.0",
  "description": "A small demo project that shows how to use webpack for client-side development in ECMAScript 6.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public/*",
    "fooprebuild": "npm run clean -s",
    "build": "npm run build:scripts -s",
    "build:scriptsfoo": "webpack -w --on-first-compile 'npm run serve' --on-compile 'echo hello'",
    "build:scripts": "webpack -w",
    "watch": "parallelshell 'npm run watch:build -s'",
    "foopreserve": "npm run build -s",
    "serve": "nodemon -w index.js -w public/bundle.js",
    "build:live": "npm run live-reload -s & npm run build:scripts -s",
    "live-reload": "live-reload --port 3001 index.js public/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shaunwest/webpack-es6-demo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shaunwest/webpack-es6-demo/issues"
  },
  "homepage": "https://github.com/shaunwest/webpack-es6-demo",
  "devDependencies": {
    "babel-core": "^5.0.8",
    "babel-loader": "^5.0.0",
    "live-reload": "^1.1.0",
    "nodemon": "^1.3.7",
    "parallelshell": "^1.1.1",
    "rimraf": "^2.3.2",
    "webpack": "^1.7.3"
  },
  "dependencies": {
    "express": "^4.12.3",
    "jquery": "^2.1.3",
    "jsdom": "^4.1.0"
  }
}
