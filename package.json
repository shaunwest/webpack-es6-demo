  "bugs": {
    "url": "https://github.com/shaunwest/webpack-es6-demo/issues"
  },
  "homepage": "https://github.com/shaunwest/webpack-es6-demo",
  "devDependencies": {
    "babel-core": "^5.0.8",
    "babel-loader": "^5.0.0",
    "colors": "^1.0.3",
    "exports-loader": "^0.6.2",
    "imports-loader": "^0.6.3",
    "livereload": "^0.3.7",
    "nodemon": "^1.3.7",
    "rimraf": "^2.3.2",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.7.3",
    "yargs": "^3.7.1"
  },
  "dependencies": {
    "custom-elements": "^1.0.2",
    "customelements": "0.0.7",
    "document-register-element": "^0.2.2",
    "express": "^4.12.3",
